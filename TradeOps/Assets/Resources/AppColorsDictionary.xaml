    <Color x:Key="AppSecondaryTint1">#FF4D5159</Color>
    <Color x:Key="AppSecondaryTint2">#FF61656B</Color>
    <Color x:Key="AppSecondaryTint3">#FF74787E</Color>
    <Color x:Key="AppSecondaryTint4">#FF888B90</Color>
