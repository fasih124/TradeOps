    <SolidColorBrush x:Key="AppPrimaryTintBrush3" Color="{StaticResource AppPrimaryTint3}" />
    <SolidColorBrush x:Key="AppPrimaryTintBrush4" Color="{StaticResource AppPrimaryTint4}" />
    <SolidColorBrush x:Key="AppPrimaryTintBrush5" Color="{StaticResource AppPrimaryTint5}" />
