    <Color x:Key="AppPrimaryShade2">#FF008A91</Color>
    <Color x:Key="AppPrimaryShade3">#FF00797F</Color>
    <Color x:Key="AppPrimaryShade4">#FF00686D</Color>
    <Color x:Key="AppPrimaryShade5">#FF00575B</Color>
    <Color x:Key="AppPrimaryShade6">#FF004548</Color>
    <Color x:Key="AppPrimaryShade7">#FF002324</Color>

    <SolidColorBrush x:Key="AppPrimaryShadeBrush1" Color="{StaticResource AppPrimaryShade1}" />
    <SolidColorBrush x:Key="AppPrimaryShadeBrush2" Color="{StaticResource AppPrimaryShade2}" />
    <SolidColorBrush x:Key="AppPrimaryShadeBrush3" Color="{StaticResource AppPrimaryShade3}" />
    <SolidColorBrush x:Key="AppPrimaryShadeBrush4" Color="{StaticResource AppPrimaryShade4}" />
    <SolidColorBrush x:Key="AppPrimaryShadeBrush5" Color="{StaticResource AppPrimaryShade5}" />
