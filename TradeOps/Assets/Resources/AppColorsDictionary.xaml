<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Base Background Color  -->
    <Color x:Key="AppBaseBackgroundColor">#FFEEEEEE</Color>
    <SolidColorBrush x:Key="AppBaseBackgroundBrush" Color="{StaticResource AppBaseBackgroundColor}" />

    <!--  Background Shades (Darker)  -->
    <Color x:Key="AppBackgroundShade1">#FFD6D6D6</Color>
    <Color x:Key="AppBackgroundShade2">#FFBEBEBE</Color>
    <Color x:Key="AppBackgroundShade3">#FFA7A7A7</Color>
    <Color x:Key="AppBackgroundShade4">#FF8F8F8F</Color>
    <Color x:Key="AppBackgroundShade5">#FF777777</Color>
    <Color x:Key="AppBackgroundShade6">#FF5F5F5F</Color>

    <SolidColorBrush x:Key="AppBackgroundShadeBrush1" Color="{StaticResource AppBackgroundShade1}" />
    <SolidColorBrush x:Key="AppBackgroundShadeBrush2" Color="{StaticResource AppBackgroundShade2}" />
    <SolidColorBrush x:Key="AppBackgroundShadeBrush3" Color="{StaticResource AppBackgroundShade3}" />
    <SolidColorBrush x:Key="AppBackgroundShadeBrush4" Color="{StaticResource AppBackgroundShade4}" />
    <SolidColorBrush x:Key="AppBackgroundShadeBrush5" Color="{StaticResource AppBackgroundShade5}" />
    <SolidColorBrush x:Key="AppBackgroundShadeBrush6" Color="{StaticResource AppBackgroundShade6}" />

    <!--  Background Tints (Lighter)  -->
    <Color x:Key="AppBackgroundTint1">#FFF0F0F0</Color>
    <Color x:Key="AppBackgroundTint2">#FFF1F1F1</Color>
    <Color x:Key="AppBackgroundTint3">#FFF3F3F3</Color>
    <Color x:Key="AppBackgroundTint4">#FFF5F5F5</Color>
    <Color x:Key="AppBackgroundTint5">#FFF8F8F8</Color>
    <Color x:Key="AppBackgroundTint6">#FFFCFCFC</Color>
    <Color x:Key="AppBackgroundTint7">#FFFFFFFF</Color>

    <SolidColorBrush x:Key="AppBackgroundTintBrush1" Color="{StaticResource AppBackgroundTint1}" />
    <SolidColorBrush x:Key="AppBackgroundTintBrush2" Color="{StaticResource AppBackgroundTint2}" />
    <SolidColorBrush x:Key="AppBackgroundTintBrush3" Color="{StaticResource AppBackgroundTint3}" />
    <SolidColorBrush x:Key="AppBackgroundTintBrush4" Color="{StaticResource AppBackgroundTint4}" />
    <SolidColorBrush x:Key="AppBackgroundTintBrush5" Color="{StaticResource AppBackgroundTint5}" />
    <SolidColorBrush x:Key="AppBackgroundTintBrush6" Color="{StaticResource AppBackgroundTint6}" />
    <SolidColorBrush x:Key="AppBackgroundTintBrush7" Color="{StaticResource AppBackgroundTint7}" />


    <!--  Base Primary Color  -->
    <Color x:Key="AppPrimaryColor">#FF00ADB5</Color>
    <SolidColorBrush x:Key="AppPrimaryBrush" Color="{StaticResource AppPrimaryColor}" />
