    <!--  Primary Tints (Lighter)  -->
    <Color x:Key="AppPrimaryTint1">#FF1AB5BC</Color>
    <Color x:Key="AppPrimaryTint2">#FF33BDC4</Color>
    <Color x:Key="AppPrimaryTint3">#FF4DC6CB</Color>
    <Color x:Key="AppPrimaryTint4">#FF66CED3</Color>
    <Color x:Key="AppPrimaryTint5">#FF80D6DA</Color>
